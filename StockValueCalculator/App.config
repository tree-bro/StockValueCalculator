  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!--label in chinese names in calculation page-->
    <add key="lblMarketPrice_zh" value="市场价 :" />
