    <add key="initValueDepressionFrequency" value="5" />
    <add key="initValueDepressionLossRate" value="99.9" />
    <add key="initValueStockHeldDuration" value="5" />
    <!--init values for the stock info page-->
    <add key="initValueKeepPreferStockList" value="true"/>
