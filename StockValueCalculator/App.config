    <add key="lblCompanyDuration_zh" value="企业存续期 :" />
    <add key="lblDiscountRate_zh" value="折现率(%) :" />
    <add key="lblNormalGrowthRate_zh" value="自然增长率(%) :" />
    <add key="lblHighSpeedGrowthRate_zh" value="高速增长率(%) :" />
